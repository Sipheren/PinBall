{
  "name": "pinball-asset-generator",
  "version": "1.0.0",
  "type": "module",
  "description": "Generate pinball game assets using Google Gemini API",
  "scripts": {
    "generate": "tsx src/index.ts generate",
    "single": "tsx src/index.ts single",
    "themes": "tsx src/index.ts list-themes",
    "models": "tsx src/index.ts list-models",
    "generate-all": "tsx src/index.ts generate-all",
    "generate-critical": "tsx src/index.ts generate-all --priority critical",
    "generate-high": "tsx src/index.ts generate-all --priority high",
    "count": "tsx src/index.ts count-assets",
    "build": "tsc"
  },
  "dependencies": {
    "@google/genai": "^1.37.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
